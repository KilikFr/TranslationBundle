{
  "name": "kilik/translation-bundle",
  "description": "Symfony Tool to help Import/Export (missing) translations",
  "keywords": ["symfony", "translation"],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Michel NAUD",
      "role": "Author"
    }
  ],
  "require": {
    "php": "^7.2",
    "symfony/yaml": "^4.0||^5.0",
    "symfony/finder": "^4.0||^5.0",
    "symfony/filesystem": "^4.0||^5.0"
  },
  "require-dev": {
    "symfony/http-kernel": "^4.0||^5.0",
    "symfony/phpunit-bridge": "^4.0||^5.0",
    "symfony/dependency-injection": "^4.0||^5.0"
  },
  "autoload": {
    "psr-4": {
      "Kilik\\TranslationBundle\\": "src"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  }
}
